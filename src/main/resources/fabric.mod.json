{
  "schemaVersion": 1,
  "id": "serverseeker",
  "version": "${version}",
  "name": "ServerSeeker",
  "description": "An addon template for the Meteor addons.",
  "authors": [
    "DAMcraft"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "de.damcraft.serverseeker.ServerSeeker"
    ]
  },
  "mixins": [
    "serverseeker.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "153,45,34"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
